{
  "name": "@yton/zp",
  "version": "0.1.0",
  "description": "Cross-platform batch archive extraction tool with multi-password support and nested extraction",
  "type": "module",
  "main": "bin/zp.js",
  "bin": {
    "zp": "./bin/zp.js"
  },
  "scripts": {
    "start": "node bin/zp.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/**/*.js bin/*.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "nodemon bin/zp.js"
  },
  "keywords": [
    "archive",
    "extraction",
    "zip",
    "rar",
    "7zip",
    "batch",
    "cli",
    "cross-platform",
    "macos",
    "windows",
    "linux"
  ],
  "author": "yton",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "7zip-bin-full": "^25.1.1",
    "7zip-min": "^2.1.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yton/zp.git"
  },
  "bugs": {
    "url": "https://github.com/yton/zp/issues"
  },
  "homepage": "https://github.com/yton/zp#readme",
  "preferGlobal": true
}
